#include <stdio.h>
int main()
{
    int num;
    printf("Enter the number: ");
    scanf("%d",&num);
    switch(num)
    {
        case 0:
        printf("Number is in the list");
        break;
        case 1:
        printf("Number is in the list");
        break;
        default:
        printf("Number isn't in the list");
    }
    return 0;
}
